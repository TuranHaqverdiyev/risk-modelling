# Example environment variables for ABB Credit Risk
# Copy to .env and fill in real values for production

# Streamlit
STREAMLIT_SECRET_KEY=your_streamlit_secret

# MinIO
MINIO_ENDPOINT=http://minio:9000
MINIO_EXTERNAL_ENDPOINT=http://localhost:9002
MINIO_ACCESS_KEY=your_minio_access_key
MINIO_SECRET_KEY=your_minio_secret_key
MINIO_BUCKET=your_bucket_name

# MLflow
MLFLOW_TRACKING_URI=http://mlflow:5000
MLFLOW_EXTERNAL_TRACKING_URI=http://localhost:5001
MLFLOW_S3_ENDPOINT_URL=http://minio:9000
MLFLOW_S3_BUCKET=credit-risk

# Other
ENV=development
